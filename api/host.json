{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information",
            "Host": "Warning",
            "Function": "Warning",
            "Host.Aggregator": "Information"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "maxTelemetryItemsPerSecond" : 20,
                "excludedTypes": "Request;Exception"
            },
            "enableLiveMetricsFilters": true
        },    
        "aggregator": {
            "batchSize": 1000,
            "flushTimeout": "00:00:30"
        }
    }
}